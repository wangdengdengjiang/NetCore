{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.1.3",
      "destination": "wwwroot/twitter-bootstrap/"
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      //cdnjs 库没有要用的东西时，可以用provider 引用其他库
      "provider": "unpkg",
      "library": "abp-web-resources@5.4.0",
      "destination": "wwwroot/lib/"
    }
  ]
}